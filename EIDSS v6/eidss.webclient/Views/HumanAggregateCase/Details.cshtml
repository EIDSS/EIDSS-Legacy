@model eidss.model.Schema.HumanAggregateCase
@{
    ViewBag.Title = @Translator.GetMessageString("titleHumanAggregateCaseTitle");
    ViewBag.Number = "H06";
    Layout = "~/Views/Shared/_DetailsLayout.cshtml";
    
}
@using bv.common.Configuration
@using eidss.web.common.Utils
@using eidss.webclient.Utils;

<script src="@Url.Content("~/Scripts/EIDSS/FlexForms.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/EIDSS/AggregateCase.js")" type="text/javascript"></script>

<script language="javascript" type="text/javascript">
    $(window).load(function () {           
        aggregateCase.toFlexFormCaseReload();
    });
    
    $(document).ready(function () {
    });
</script>

@if (BaseSettings.TranslationMode){
    <input type='hidden' id='aspclassname' value='@GetType().FullName' />
}

@using (Html.BeginForm())
{       
    @Html.IdentificationAndHeartbeat("idfAggrCase", Model.idfAggrCase)   
      
    <div class="tabPanelContent bordered">
        @Html.Partial("../HumanAggregateCase/DetailsContent", Model)
    </div>    
}
