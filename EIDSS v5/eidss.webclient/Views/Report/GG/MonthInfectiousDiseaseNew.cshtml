@model eidss.model.Reports.GG.MonthInfectiousDiseaseModel

@{
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
}


@using (Html.BeginForm())
{
    @Html.Partial("Common/BaseFilter", Model)

    @Html.Partial("Common/BaseDateFilter", Model)

    @Html.Partial("GG/RayonFilter", Model.RayonFilter)

    <table id="SignatureTable" >
       
        <tr>
            <td>
                @Html.CheckBoxFor(m => m.AddSignature)
                @Html.LabelFor(m => m.AddSignature)
            </td>
            <td colspan="2"></td>
        </tr>
    </table>

    @Html.Partial("Common/ExportButtons", Model)
}