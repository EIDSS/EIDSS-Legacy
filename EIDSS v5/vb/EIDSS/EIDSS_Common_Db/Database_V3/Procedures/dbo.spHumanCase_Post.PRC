exec spHumanCase_Post 
	 @idfCase
	,@idfOutbreak
	,@datEnteredDate
	,@strCaseID
	,@idfsCaseProgressStatus
	,@idfsFinalState
	,@idfsHospitalizationStatus
	,@idfsHumanAgeType
	,@idfsYNAntimicrobialTherapy
	,@idfsYNHospitalization
	,@idfsYNSpecimenCollected
