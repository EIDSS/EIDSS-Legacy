	,@idfsFinalCaseStatus
	,@idfSentByOffice
	,@idfReceivedByOffice
	,@idfInvestigatedByOffice
	,@idfPointGeoLocation
	,@idfEpiObservation
	,@idfsEPIFormTemplate
	,@idfCSObservation
	,@idfsCSFormTemplate
	,@datNotificationDate
	,@datCompletionPaperFormDate
	,@datFirstSoughtCareDate
	,@datModificationDate
	,@datHospitalizationDate
	,@datFacilityLastVisit
	,@datExposureDate
	,@datDischargeDate
	,@datOnsetDate
	,@datInvestigationStartDate
	,@datTentativeDiagnosisDate
	,@datFinalDiagnosisDate
	,@strNote
	,@strCurrentLocation
	,@strHospitalizationPlace
