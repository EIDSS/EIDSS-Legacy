    Function gvAntimicrobialTherapy_ValidateRow(ByVal rowHandle As Integer) As Boolean
        If (Not gvAntimicrobialTherapy Is Nothing) AndAlso (rowHandle >= 0) AndAlso (rowHandle < gvAntimicrobialTherapy.RowCount) Then
            Dim r As DataRow = gvAntimicrobialTherapy.GetDataRow(rowHandle)
            If Utils.IsEmpty(r("strAntimicrobialTherapyName")) Then
                MessageForm.Show(EidssMessages.Get("mbFillOrDeleteAntibiotic", "Some records from the table of antibiotic or antiviral therapy are not defined. Please define or delete undefined records."))
                Return False
            End If
        End If
        Return True
    End Function

    Function gvAntimicrobialTherapy_Validate() As Boolean
        If (Not gvAntimicrobialTherapy Is Nothing) AndAlso (gvAntimicrobialTherapy.RowCount > 0) Then
            Dim rowHandle As Integer = 0
            While (rowHandle < gvAntimicrobialTherapy.RowCount)
                Dim r As DataRow = gvAntimicrobialTherapy.GetDataRow(rowHandle)
                If (r.RowState = DataRowState.Added) OrElse (r.RowState = DataRowState.Modified) Then
                    If Utils.IsEmpty(r("strAntimicrobialTherapyName")) Then
                        MessageForm.Show(EidssMessages.Get("mbFillOrDeleteAntibiotic", "Some records from the table of antibiotic or antiviral therapy are not defined. Please define or delete undefined records."))
                        Return False
                    End If
                End If
                rowHandle = rowHandle + 1
            End While
        End If
        Return True
    End Function

