    Public Overrides Function Post(Optional ByVal postType As bv.common.PostType = bv.common.PostType.FinalPosting) As Boolean
        Dim bRet As Boolean = False

        DataEventManager.Flush()

        If IsSearchMode() OrElse _
           (((HumanCaseDbService Is Nothing) OrElse (Not HumanCaseDbService.IsNewObject)) AndAlso _
            (Not HasChanges())) Then
            Return True
        End If
        If (baseDataSet.Tables(HumanCase_DB.tlbHumanCase).Rows.Count > 0) Then
