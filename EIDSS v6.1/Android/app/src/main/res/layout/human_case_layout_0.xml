        <!--section to paste end-->
        <!--CreateDate-->
        <LinearLayout
            style="@style/DetailLayout.Port">
            <TextView
                style="@style/DetailItem.Header.Readonly"
                android:text="@string/Created" />
            <LinearLayout
                style="@style/DetailLayout.Date">
                <TextView
                    android:id="@+id/datCreateDate"
                    style="@style/DetailItem.Date.Readonly"
                    />
            </LinearLayout>
            <TextView
                style="@style/Separator"/>
        </LinearLayout>
    </LinearLayout>
</ScrollView>