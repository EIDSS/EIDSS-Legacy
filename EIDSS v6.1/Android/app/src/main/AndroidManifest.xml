        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/ic_launcher"
        android:label="@string/ApplicationName"
        android:theme="@style/CustomActionBarTheme"
        android:name=".App"
        >
        <activity
            android:name="com.bv.eidss.StartActivity"
            android:label="@string/ApplicationName" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bv.eidss.SetLocalPasswordActivity" />
        <activity android:name="com.bv.eidss.ChangeLocalPasswordActivity">
            android:parentActivityName="com.bv.eidss.SettingsActivity" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bv.eidss.SettingsActivity" />
        </activity>
        <activity android:name="com.bv.eidss.LocalLoginActivity" />
        <activity android:name="com.bv.eidss.SettingsActivity" />
        <activity android:name="com.bv.eidss.HumanCaseActivity">
