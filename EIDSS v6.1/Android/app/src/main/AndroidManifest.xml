        <activity android:name="com.bv.eidss.HumanCaseListActivity" />
        <activity android:name="com.bv.eidss.VetCaseListActivity">
        </activity>
        <activity android:name="com.bv.eidss.ASSessionListActivity">
        </activity>
        <activity android:name="com.bv.eidss.VetCaseActivity" android:windowSoftInputMode="adjustPan">
            android:parentActivityName="com.bv.eidss.VetCaseListActivity" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bv.eidss.VetCaseListActivity" />
        </activity>
        <activity android:name="com.bv.eidss.AnimalActivity" android:windowSoftInputMode="adjustPan">
